{
  "version": 2,
  "builds": [
    {
      "src": "errorManagementSystem/errorManagementSystem/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "errorManagementSystem/errorManagementSystem/wsgi.py"
    }
  ]
}
